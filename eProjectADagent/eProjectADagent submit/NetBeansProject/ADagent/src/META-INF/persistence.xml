<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  <persistence-unit name="ADagentPU" transaction-type="RESOURCE_LOCAL">
    <provider>oracle.toplink.essentials.PersistenceProvider</provider>
    <class>adagent.TOrder</class>
    <class>adagent.TEmployee</class>
    <class>adagent.TContType</class>
    <class>adagent.TPageType</class>
    <class>adagent.TSizeType</class>
    <properties>
      <property name="toplink.jdbc.user" value="sa"/>
      <property name="toplink.jdbc.password" value="aptech"/>
      <property name="toplink.jdbc.url" value="jdbc:sqlserver://127.0.0.1:1433;databaseName=ADagent"/>
      <property name="toplink.jdbc.driver" value="com.microsoft.sqlserver.jdbc.SQLServerDriver"/>
    </properties>
  </persistence-unit>
</persistence>
