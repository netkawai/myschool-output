<?xml version="1.0"?>

<configuration>
  
    <appSettings/>
  <connectionStrings>
    <add name="y510.SRC.dbo" connectionString="Data Source=SONY-VAIO\SQLEXPRESS;Initial Catalog=SRC;Persist Security Info=True;User ID=sa;Password=123" />
    <add name="SRCConnection" connectionString="Data Source=SONY-VAIO\SQLEXPRESS;Initial Catalog=SRC;Persist Security Info=True;User ID=sa;Password=123"
